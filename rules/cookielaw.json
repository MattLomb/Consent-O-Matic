{
	"$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "cookielaw": {
        "detectors": [
            {
                "presentMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "#cookie-law-bar"
                    }
                },
                "showingMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "#cookie-law-bar"
                    }
                }
            }
        ],
        "methods": [
            {
                "action": {
                    "type": "hide",
                    "target": {
                        "selector": "#cookie-law-bar"
                    }
                },
                "name": "HIDE_CMP"
            },
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "name": "SAVE_CONSENT"
            },
            {
                "name": "UTILITY"
            }
        ]
    }
}